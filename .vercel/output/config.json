{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/_image/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/404/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/projects/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/projects(?:/(.*?))?/?$",
			"dest": "_middleware"
		},
		{
			"src": "^/$",
			"dest": "_middleware"
		},
		{
			"src": "^/.*$",
			"dest": "_middleware",
			"status": 404
		}
	]
}